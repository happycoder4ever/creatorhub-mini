generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model User {
  id        String      @id @default(cuid())
  wallet    String      @unique
  createdAt DateTime    @default(now())
  nftAccess NFTAccess[]
}

model NFTAccess {
  id        String   @id @default(cuid())
  tokenId   String
  active    Boolean  @default(true)
  createdAt DateTime @default(now())
  userId    String
  user      User     @relation(fields: [userId], references: [id])
}
